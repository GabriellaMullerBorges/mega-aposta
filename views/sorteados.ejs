<!DOCTYPE html>
<html>
<head>
  <title>Mega sena</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
</head>
<body>
  <header>
    <button onclick="window.location.href = '/home';" class="btn btn-primary">Home</button>

    <h1>SORTEIO REALIZADO</h1>
  </header>

  <section>
    <h3>Números Sorteados</h3>
    <p>Números: <%= numerosSorteados.join(', ') %></p>
    <% if (vencedores.length > 0) { %>
      <h3>Apostadores Vencedores</h3>
      <ul>
        <% vencedores.forEach(function(vencedor) { %>
          <li>Apostador: <%= vencedor.nome %>, CPF: <%= vencedor.cpf %></li>
        <% }); %>
      </ul>
    <% } else { %>
      <p>Nenhum vencedor encontrado após 25 tentativas.</p>
    <% } %>
  </section>

  <footer>
    <p> Gabriella Müller Borges</p>
  </footer>

  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
</body>
</html>
